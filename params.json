  "body": "<html>\r\n\r\n<head>\r\n\r\n<title>Canadian Open Data Websites</title>\r\n\r\n<link rel=\"stylesheet\" href=\"http://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.3/leaflet.css\" />\r\n<link rel=\"stylesheet\" href=\"styles.css\" />\r\n\r\n<script src=\"http://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.3/leaflet.js\"></script>\r\n<script src=\"https://code.jquery.com/jquery-2.1.1.min.js\"></script>\r\n\r\n<script src=\"//cdn.jsdelivr.net/leaflet.esri/1.0.0/esri-leaflet.js\"></script>\r\n\r\n<script src=\"MarkerCluster/leaflet.markercluster.js\"></script>\r\n\r\n</head>\r\n\r\n<style>\r\n  body {\r\n    font-family: \"Helvetica Neue\",Helvetica,\"Segoe UI\",Arial,freesans,sans-serif;\r\n  }\r\n</style>\r\n<body>\r\n\r\n  <h2>Madame de Thebes film distribution, 1915-1918</h2>\r\n  <p>blah blah</p>\r\n\r\n<div id=\"map\"></div>\r\n\r\n<script>\r\n\r\nvar layer = L.tileLayer('http://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png', {\r\n  attribution: '&copy; <a href=\"http://www.openstreetmap.org/copyright\">OpenStreetMap</a> contributors, &copy; <a href=\"http://cartodb.com/attributions\">CartoDB</a>'\r\n});\r\n\r\nvar map = L.map('map', {\r\n  scrollWheelZoom: false,\r\n  center: [-15.0, 15.0],\r\n  zoom: 2\r\n});\r\n\r\nmap.addLayer(layer);\r\n\r\n//add points\r\n$.getJSON('destinations_apr13_CartoDB.geojson',function(data){\r\n    L.geoJson(data,{\r\n\t\t\tpointToLayer: function(feature,latlng){\r\n\t\t\t\tvar marker = L.circleMarker(latlng, { //code from http://cartographicperspectives.org/index.php/journal/article/view/cp76-donohue-et-al/1307\r\n\t\t\t\t\tfillColor: \"#708598\",\r\n\t\t\t\t\tcolor:\"#537898\",\r\n\t\t\t\t\tweight: 1,\r\n\t\t\t\t\tfillOpacity: 0.8\r\n\t\t\t\t});\r\n        //marker.bindPopup(\"<strong>\" + feature.properties.name + \"</strong><br></br>\" + feature.properties.denom_full + \" school in \" + feature.properties.town + \", \" + feature.properties.opened + \" - \" + feature.properties.closed);\r\n\t\t\t\treturn marker\r\n      }\r\n  }).addTo(map);\r\n});\r\n\r\n//add lines\r\n$.getJSON('Lines_Frihavn2.geojson',function(Lines){\r\n  L.geoJson( Lines,{\r\n\t\tstyle: function (feature){\r\n      var lineweight;\r\n        switch (feature.properties.MdeT_Total) {\r\n          case '2':\t\t\tlineweight = 3; break;\r\n          case '3':\t\t\tlineweight = 4; break;\r\n          case '5':\t\t\tlineweight = 6; break;\r\n          case '6':\t\t\tlineweight = 7; break;\r\n          case '7':\t\t\tlineweight = 8; break;\r\n          case '8':\t\t\tlineweight = 9; break;\r\n          case '9':\t\t\tlineweight = 10; break;\r\n          case '10':\t\tlineweight = 11; break;\r\n          default: \t\t\tlineweight = 2;\r\n        }\r\n\t\t\treturn {color: \"#08184D\", weight: lineweight};\r\n\t\t},\r\n\t\tonEachFeature: function(feature, layer){\r\n      layer.bindPopup(feature.properties.MdeT_Total + \" copies sent from \" + feature.properties.Origin1 + \" to \" + feature.properties.Destinatio);\r\n      return layer;\r\n\t\t},\r\n\t}).addTo(map);\r\n});\r\n\r\nL.control.scale().addTo(map);\r\n\r\n</script>\r\n\r\n</body>\r\n\r\n</html>\r\n",
